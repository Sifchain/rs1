{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/continueStories",
      "schedule": "*/15 * * * *"
    }
     {
      "path": "/api/cron/savePollResults",
      "schedule": "*/15 * * * *"
    }
  ]
}
